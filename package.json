{
  "name": "hmr",
  "version": "0.6.8-rc7258e89d.1",
  "description": "chip-in HMR(HTTP Message Router) server",
  "keywords": [
    "chip-in"
  ],
  "bugs": {
    "url": "https://github.com/chip-in/hmr/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node lib/main.js",
    "cleanbuild": "npm run clean && npm run build",
    "build": "npm run lint && babel src -s inline -d lib/",
    "clean": "rimraf lib",
    "lint": "eslint src",
    "test": "mocha",
    "debug": "node --inspect --inspect-brk --inspect-port=9230 lib/main.js",
    "watch": "npm run clean && babel src -s inline -d lib/ -w"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cli-table3": "^0.6.0",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "connect-timeout": "^1.9.0",
    "content-type": "^1.0.4",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "ejs": "~2.5.6",
    "express": "^4.18.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.1.0",
    "mongo-parse": "^2.0.3",
    "morgan": "^1.9.1",
    "semaphore": "^1.1.0",
    "serve-favicon": "^2.4.5",
    "socket.io": "^4.6.1",
    "ua-parser-js": "^0.7.28",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "mocha": "^9.2.1",
    "rimraf": "^3.0.2"
  },
  "bin": {
    "hmradmin": "./bin/hmradmin.js"
  }
}
