{
  "name": "hmr",
  "version": "0.0.4",
  "description": "chip-in HMR(HTTP Message Router) server",
  "keywords": [
    "chip-in"
  ],
  "bugs": {
    "url": "https://github.com/chip-in/hmr/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "node lib/main.js",
    "cleanbuild": "npm run clean && npm run build",
    "build": "babel src -s inline -d lib/",
    "clean": "del-cli lib",
    "debug": "node --inspect --inspect-brk --inspect-port=9230 lib/main.js",
    "watch": "npm run clean && babel src -s inline -d lib/ -w"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "helmet": "^3.8.1",
    "jsonwebtoken": "^8.1.0",
    "mongo-parse": "^2.0.3",
    "morgan": "~1.8.1",
    "semaphore": "^1.1.0",
    "serve-favicon": "~2.4.2",
    "socket.io": "^2.0.3",
    "ua-parser-js": "^0.7.17",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cpx": "^1.5.0",
    "del-cli": "^1.1.0"
  }
}
